{
  "name": "todo-list",
  "version": "1.0.0",
  "description": "Example of a graphql todo-list server",
  "scripts": {
    "start": "NODE_ENV=development nodemon src/index.js --exec babel-node",
    "build": "babel src --ignore __tests__ --out-dir dist/",
    "serve": "NODE_ENV=production node dist/index.js",
    "watch": "jest --watch --runInBand",
    "test": "jest --forceExit --runInBand",
    "check": "flow check",
    "travis": "npm run check && npm run lint && npm run test",
    "lint": "eslint src"
  },
  "engines": {
    "node": "4.x || 5.x || 6.x || 7.x",
    "npm": "2.x || 3.x"
  },
  "keywords": [
    "todo-list",
    "graphQL",
    "API",
    "node.js"
  ],
  "author": "Dirk-Jan Rutten <dirkjanrutten@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "dataloader": "^1.3.0",
    "express": "^4.14.0",
    "express-graphql": "^0.6.3",
    "graphql": "^0.9.1",
    "graphql-relay": "^0.5.1",
    "pg": "^6.1.2",
    "sequelize": "^3.28.0"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^18.0.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "eslint": "^3.15.0",
    "eslint-plugin-babel": "^4.0.1",
    "eslint-plugin-flowtype": "^2.29.1",
    "flow-bin": "^0.39.0",
    "jest": "^18.0.0",
    "nodemon": "^1.9.2"
  },
  "standard": {
    "parser": "babel-eslint",
    "plugins": [
      "flowtype"
    ],
    "globals": [
      "describe",
      "before",
      "beforeEach",
      "beforeAll",
      "after",
      "afterEach",
      "afterAll",
      "it",
      "expect"
    ],
    "ignore": [
      "flow-typed/"
    ]
  }
}
